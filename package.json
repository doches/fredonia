{
  "name": "fredonia",
  "version": "0.0.0",
  "description": "A library of React components & styles for Quail",
  "main": "./dist/fredonia.js",
  "types": "./dist/**/*.d.ts",
  "files": "./dist/**/*",
  "author": "trevor@quailhq.com",
  "license": "MIT",
  "scripts": {
    "build": "npm-run-all clean lint compile copy",
    "lint": "tslint --config tslint.json 'src/**/*.tsx' --fix",
    "clean": "rm -rf dist/*",
    "compile": "webpack --config webpack.config.js",
    "copy": "cp src/variables.less dist/",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@storybook/addon-backgrounds": "^4.1.3",
    "@storybook/addon-knobs": "^4.1.3",
    "@storybook/addon-options": "^4.1.3",
    "@storybook/react": "^4.1.3",
    "@types/classnames": "^2.2.6",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.17",
    "@types/storybook__react": "^4.0.0",
    "babel-loader": "^8.0.4",
    "css-loader": "^2.0.1",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.1",
    "tslint": "^5.12.0",
    "tslint-loader": "^3.5.4",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "peerDependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/pro-regular-svg-icons": "^5.6.3",
    "@fortawesome/pro-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "classnames": "^2.2.6",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  }
}
